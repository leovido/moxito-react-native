    "check:fix:unsafe": "npx @biomejs/biome check --write --unsafe .",
    "format:check": "npx @biomejs/biome format .",
    "test:unit": "jest --coverage --verbose --detectOpenHandles --forceExit",
    "test:unit:watch": "jest --watchAll"
