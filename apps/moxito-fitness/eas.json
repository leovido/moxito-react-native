{
  "cli": {
    "version": ">= 14.5.0",
    "appVersionSource": "remote",
    "requireCommit": true
  },
  "build": {
    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium",
        "simulator": false
      }
    },
    "preview": {
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium"
      },
      "developmentClient": false,
      "channel": "preview"
    },
    "production": {
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {}
  },
  "credentials": {
    "ios": {
      "appStoreConnect": {
        "apiKeyId": {"env": "APPSTORE_API_KEY_ID"},
        "apiKeyIssuerId": {"env": "APPSTORE_ISSUER_ID"},
        "apiKeyPath": {"env": "APPSTORE_API_KEY_PATH"}
      }
    }
  }
}
